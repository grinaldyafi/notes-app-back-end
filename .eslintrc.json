{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style":"off",
        "no-multiple-empty-lines":"off",
        "eol-last":"off",
        "no-console":"off",
        "indent":"off",
        "space-before-function-paren":"off"
    }
}
